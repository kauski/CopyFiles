<Window x:Class="Harjoitus.Completed"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Harjoitus"
        mc:Ignorable="d"
        Title="Completed" Height="450" Width="800">
    <Grid>
        <TextBlock Text="Completed" FontSize="20" HorizontalAlignment="Center" VerticalAlignment="top" Grid.Row="0"/>
        <Button Content="Quit" Click="Exit" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 0 20" Width="100"/>
    </Grid>
</Window>
